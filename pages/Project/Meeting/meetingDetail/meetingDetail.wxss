
.view-container .title-view {
  border-bottom: 2rpx solid #f2f2f2;
  height: 120rpx;
  padding: 16rpx 0rpx;
  justify-content: space-between;
}

.view-container .content-view {
  height: 200rpx;
  padding: 16rpx 0rpx;
}

.view-container .title-view .title {
  font-size: 40rpx;
}

.view-container .title-view .content {
  font-size: 34rpx;
}

.view-container image {
  height: 40rpx;
  width: 40rpx;
}

.view-container .item-view {
  border-bottom: 2rpx solid #f2f2f2;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.view-container .desc {
  padding-left: 30rpx;
  font-size: 34rpx;
}

.member-view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.desc.delete-text {
  color: #e64340;
}
