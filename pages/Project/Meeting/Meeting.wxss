/* pages/Project/Meeting/Meeting.wxss */
.Meeting-container{
  width: 100%;
  display: flex;
  flex-direction: column;
}


.Meeting-container .noitem{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top:260rpx;
}   

.Meeting-container .noitem .noitem-icon{
  width:130rpx;
  height:130rpx;
}

.Meeting-container .noitem  .noitem-title{
  font-size: 40rpx;
  color: #888888;
  margin-top:20rpx;
}

.Meeting-container .noitem  .noitem-content{
  font-size: 30rpx;
  color: #b2b2b2;
  margin-top:10rpx;
}

.Meeting-container .noitem  .noitem-create{
  color: #fff;
  background: #09BB07;
  margin-top: 40rpx;
}


.Meeting-container .title{
    color: #888888;
    margin-left: 40rpx;
    margin-top: 30rpx;
    margin-bottom: 16rpx;
    font-size: 28rpx;
}

.Meeting-container .item{
    background: #fff;
    color: #000;
    width: 100%;
    height: 120rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.Meeting-container .item .item-first{
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: space-around;
    margin-left: 40rpx;
}

.Meeting-container .item .item-first .item-date{
    font-size: 34rpx;
    font-weight: bold;
}

.Meeting-container .item .item-first .item-time{
    font-size: 26rpx;
    color: #888888;
    margin-top:3rpx;
}

.Meeting-container .item .item-second{
    font-size: 34rpx;
}

.Meeting-container .buildicon{
  position: fixed;
  left: 640rpx;
  top: 1000rpx;
  width: 80rpx;
  height: 80rpx;
}