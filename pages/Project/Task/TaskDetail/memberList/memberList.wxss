/* pages/memberList/memberList.wxss */
.memberList-container{
    width:100%; 
    font-size: 34rpx;
    display: flex;
    flex-direction: column;
}

.memberList-container .title{
    color: #888888;
    margin-left: 40rpx;
    margin-top: 30rpx;
    margin-bottom: 16rpx;
}

.memberList-container .item{
    background: #fff;
    color: #000;
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.memberList-container .item .item-one{
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.memberList-container .item .item-one .item-icon{
    background: #888888;
    width: 48rpx;
    height: 48rpx;
    margin-left: 40rpx;
    border-radius: 20rpx;
}

.memberList-container .item .item-one .item-text{
    margin-left: 30rpx;
}

.memberList-container .item .item-checkbox{
    margin-right: 20rpx;
}

.memberList-container .addMember{
  width: 670rpx;
  height: 70rpx;
  margin-left: 60rpx;
  border-radius:16rpx;
  border: 0rpx;
  line-height: 65rpx;
  margin-top: 300rpx;
  color: #fff;
  background: #09BB07;
  font-size: 40rpx;
}

.memberList-container .deleteMember{
  width: 670rpx;
  height: 70rpx;
  margin-left: 60rpx;
  border-radius:16rpx;
  border: 0rpx;
  line-height: 65rpx;
  margin-top: 15rpx;
  color: #fff;
  background: #E64340;
  font-size: 40rpx;
}