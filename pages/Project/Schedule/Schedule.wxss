/* pages/Project/Schedule/Schedule.wxss */

.Schedule-container{
  width: 100%;
  display: flex;
  flex-direction: column;
}

.Schedule-container .noitem{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top:260rpx;
}   

.Schedule-container .noitem .noitem-icon{
  width:130rpx;
  height:130rpx;
}

.Schedule-container .noitem  .noitem-title{
  font-size: 40rpx;
  color: #888888;
  margin-top:20rpx;
}

.Schedule-container .noitem  .noitem-content{
  font-size: 30rpx;
  color: #b2b2b2;
  margin-top:10rpx;
}

.Schedule-container .noitem  .noitem-create{
  color: #fff;
  background: #09BB07;
  margin-top: 40rpx;
}

.Schedule-container .title{
    color: #888888;
    margin-left: 40rpx;
    margin-top: 30rpx;
    margin-bottom: 16rpx;
    font-size: 28rpx;
}

.Schedule-container .item{
    background: #fff;
    color: #000;
    width: 100%;
    height: 165rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.Schedule-container .item .item-first{
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: space-around;
    font-size: 34rpx;
    width:11%;

}

.Schedule-container .item .item-first .item-time{
    font-weight: bold;
}

.Schedule-container .item .item-first .item-connect{
  color: #b2b2b2;
}

.Schedule-container .item .item-second{
    font-size: 30rpx;
    width:9%;

}

.Schedule-container .item .item-third{
    font-size: 28rpx;
    color: #b2b2b2;
    width: 30%;
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
}

.Schedule-container .buildicon{
  position: fixed;
  left: 640rpx;
  top: 1000rpx;
  width: 80rpx;
  height: 80rpx;
}