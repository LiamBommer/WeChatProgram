<!--pages/buildProject/buildProject.wxml-->
<view class='buildTask-container'>

    <form bindsubmit='BuildProject'>
      <!-- 列表项 -->
    <view class='item'>
        <view class='item-title'>项目名称</view>
        <input type='text' class='item-content' name='title' placeholder-class='placeholder' placeholder='输入项目名称...' maxlength='20'></input>
    </view>
    <!-- 列表项 -->
    <view class='item'>
        <view class='item-title'>项目描述</view>
        <input type='text' class='item-content' name='desc' placeholder-class='placeholder' placeholder='输入项目描述...'></input>
    </view>

    <button class='submit' size='default' form-type='submit' disabled='{{btn_disabled}}' plain style="background-color: #09bb07; " loading='{{btn_loading}}' >完成</button>
    </form>
    

</view>


