<!--pages/buildProject/buildProject.wxml-->
<view class='buildTask-container'>

    <form bindsubmit='BuildProject'>
      <!-- 列表项 -->
    <view class='item'>
        <!-- <view class='item-title'>名称</view> -->
        <input type='text' class='item-content' name='title' placeholder-class='placeholder' placeholder='名称：女生节活动、荧光夜跑...' maxlength='20'></input>
    </view>
    <!-- 列表项 -->
    <view class='item'>
        <!-- <view class='item-title'>描述</view> -->
        <input type='text' class='item-content' name='desc' placeholder-class='placeholder' placeholder='描述：为女生庆祝、宣扬运动...'></input>
    </view>

    <button id='submit' class='submit' size='default' form-type='submit' disabled='{{btn_disabled}}' plain style="background-color: #09bb07; " loading='{{btn_loading}}' >完成</button>
    </form>
      

</view>


